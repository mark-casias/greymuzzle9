<div class="programs">
  <div class="programs__content">
    {% include "@atoms/text/headings/_heading.twig" with {
      heading_level: 2,
      heading_base_class: "label",
      heading_blockname: "programs",
      heading: label
    } %}
    <div class="programs__header">
      {{ heading }}
    </div>
    <div class="programs__items">
    {% block field_images %}
      {% for item in items %}
        {% embed "@molecules/program-card/program-card.twig" with {
          content: item.content,
        } %}
        {% endembed %}
      {% endfor %}
    {% endblock %}
    </div>
  </div>
</div>
